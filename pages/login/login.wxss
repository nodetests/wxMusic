.header{
 width: 100%;
 display: flex;
 justify-content: space-between;
}
.header .emailLogin{
    width:50%;
    height: 60rpx;
    text-align: center;
    line-height: 60rpx;
}

.header .telLogin{
    width:50%;
    height: 60rpx;
    text-align: center;
    line-height: 60rpx;
}
.line{
    width: 150rpx;
    height: 4rpx;
    background: #f46;
    margin-left: 110rpx;
}
.msg{
    width: 100%;
    margin-top: 20rpx;
}
.msg .inputs{
    width: 100%;
    height: 80rpx;
   display: flex;
   justify-content: flex-start;
}

.msg .inputs .loginImg image{
    width: 80rpx;
    height: 80rpx;
}
.msg .inputs .username{
    width: 120rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
}
.msg input{
    width: 400rpx;
    height: 80rpx;
    background: #fefeff;
}
.bottom{
    display: flex;
    justify-content: space-between;
}
.bottom button{
    background: #f46;
    color: #fff;
}
