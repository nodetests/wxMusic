.item{
    width: 100%;
    display: flex;
    justify-content: flex-start;
    margin-top: 20rpx;
}
.item .img{
    margin-top: 10rpx;
}
.item .img image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;

}
.item .msg {
    margin-left: 20rpx;
}
.item .msg .nickname{
    font-size: 24rpx;
    color: #A3A3A3;
    margin-top: 10rpx;
}
.item .msg  .programName{
    font-size: 28rpx;
    color: #414040;
    margin-top: 10rpx;
}
.item .msg .content{
    font-size: 30rpx;
    color: #252525;
    margin-top: 20rpx;
}