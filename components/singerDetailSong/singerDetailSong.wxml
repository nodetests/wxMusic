<view class="box">
    <view class="margin-box">
        <view class="newsong" >
            <view class="item" wx:for="{{artSong}}" wx:key="index">
                <view class="img" >
                    <image class="" src="{{item.al.picUrl}}" ></image>
                </view>
                <view class="song" >
                    <view class="artist" >
                        <view class="artistName" >
                            {{item.name}}
                        </view>
                          <view class="introduce" >
                            {{item.name}}-{{main.name}}
                          </view>  
                    </view>
                </view>
                <view class="play" >
                    <image class="" src="../../assets/images/nplay.png" ></image>
                </view>
             </view>
         </view>
    </view>
</view>
