.album{
    width: 100%;
}
.album .albumItem{
    display: flex;
    justify-content: flex-start;
    margin-top: 20rpx; 
}
.albumItem .img image{
    width: 150rpx;
    height: 150rpx;
    border-radius: 20rpx;
}
.albumItem .msg{
    height: 150rpx;
    margin-left: 20rpx;
}
.albumItem .msg .name1{
    font-size: 32rpx;
    font-weight: 600;
    height: 100rpx;
    line-height: 100rpx;
}
.albumItem .msg .name2{
    height: 50rpx; 
    font-size: 23rpx;
    color: #737373 ;
}
.headerTitle{
    font-size: 30rpx;
    height: 50rpx;
    font-weight: 600;
    line-height: 50rpx;
    margin-bottom: 20rpx;
}
.more{
    width: 100%;
    font-size: 28rpx;
    text-align: center;
    margin-top: 20rpx;
    margin-bottom: 20rpx;
}