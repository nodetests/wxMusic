.box{
    width: 100%;
    height: 100%;
}
.border-t{
    border-top: 2rpx solid #efefef;
} 
.border-b{
    border-bottom: 2rpx solid #efefef;
}
.margin-box{
width: 96%;
height: 100%;
margin-left: 2%;
}
.clear:after{
    content: '';        
    display:block;         
    clear: both;  
}
::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}
  